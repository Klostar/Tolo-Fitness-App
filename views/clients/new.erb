<form method="post" action "/clients">
  <label for="first-name-field">First Name:</label>
  <input
  type = "text"
  name = "first_name"
  id="first-name-field"
  />

  <label for"last-name-field">Last Name:</label>
  <input
  type = "text"
  name = :last_name
  id = "last-name-field"
  />

  <label for"age-field">Age:</label>
  <input
  type ="number"
  name="age"
  id="age-field"
  />

  <input type="submit" value ="Add Client">
</form>
